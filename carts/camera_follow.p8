--camera follow
--patreon.com/mboffin

--tab 0:basic game loop
--tab 1:player functions
--tab 2:collision functions
--tab 3:camera follow function

--only code related to camera
--following is commented in this
--cart. pixel_movement.p8.png
--has comments for the rest of
--the code.

function _init()
	make_player()
end

function _update()
	move_player()
end

function _draw()
	cls()
	
	--center the camera on the
	--player's position
	camera_follow()
	
	--draw the map from tile 0,0
	--at screen coordinate 0,0 and
	--draw 128 tiles wide, 32 tall
	map(0,0,0,0,128,32)
	
	--draw the player's sprite at
	--p.x,p.y
	spr(p.sprite,p.x,p.y)
end
-->8
--player functions

function make_player()
	p={}	
	p.x=56
	p.y=56	
	p.dx=0
	p.dy=0
	p.w=7
	p.h=7	
	p.sprite=5
end

function move_player()
	if (btn(⬅️)) p.dx-=1
	if (btn(➡️)) p.dx+=1
	if (btn(⬆️)) p.dy-=1
	if (btn(⬇️)) p.dy+=1

 if (can_move(p.x+p.dx,p.y,p.w,p.h)) then
	 p.x+=p.dx
 end
 
 if (can_move(p.x,p.y+p.dy,p.w,p.h)) then
		p.y+=p.dy
	end

	p.dx,p.dy=0,0
end
-->8
--collision functions

function can_move(x,y,w,h)		
 if (solid(x,y)) return false
 if (solid(x+w,y)) return false
 if (solid(x,y+h)) return false
 if (solid(x+w,y+h)) return false
	return true
end

function solid(x,y)
 local map_x=flr(x/8)
 local map_y=flr(y/8)
 local map_sprite=mget(map_x,map_y)
 local flag=fget(map_sprite)
 return flag==1
end
-->8
--camera follow function

--this function will move the
--camera to always put the
--player at the center.
function camera_follow()
	
	--the camera is usually pinned
	--to screen coordinate 0,0.
	--when you change the camera
	--position, pixel coordinates
	--you draw to stop matching
	--up to the actual pixels on
	--the screen. so if we move
	--the camera to 30,10 then a
	--pset(100,100,1) will draw at
	--the screen coordinate 70,90.

	--since the camera position is
	--where the top-left corner of
	--the screen is, if we want to
	--keep the player in the middle	
	--of the screen, we need to
	--set the camera x,y to the
	--player's x,y minus 60.
	cam_x=p.x-60
	cam_y=p.y-60
	
	--if the player is on the
	--edge of the map, keeping
	--player centered means you'll
	--see past the edge of the map.
	--we can use the mid() function
	--to make sure the camera
	--position can't move past
	--coordinates that would show
	--past the edge of the map.
	cam_x=mid(0,cam_x,896)
	cam_y=mid(0,cam_y,128)
	
	--change the camera position
	camera(cam_x,cam_y)
	
end
